#include<stdio.h>
#include<stdlib.h>
int a[50][50],visited[50],n;
int q[50],front=0,rear=-1;
void bfs(int start)
{
    int i,cur;
    visited[start]=1;
    q[++rear]=start;
    printf(" %d",start);
    while(front<=rear)
    {
        cur=q[front++];
        for(i=1;i<=n;i++)
        {
            if(a[cur][i]==1&&visited[i]==0)
            {
                visited[i]=1;
                q[++rear]=i;
                printf(" %d",i);
            }
        }
    }
}
void dfs(int v)
{
    int i;
    visited[v]=1;
    printf(" %d",v);
    for(i=1;i<=n;i++)
    {
        if(a[v][i]==1&&visited[i]==0)
        {
            dfs(i);
        }
    }
}
int main()
{
    int ch,start,i,j;
    printf("Enter number of vertices:");
    scanf("%d",&n);
    printf("Enter Adjacency matrix:\n");
    for(i=1;i<=n;i++)
        for(j=1;j<=n;j++)
        scanf("%d",&a[i][j]);
    printf("Enter starting vertex:");
    scanf("%d",&start);
    printf("\n1.BFS");
    printf("\n2.DFS");
    printf("\n3.EXIT");
    printf("Enter your choice:");
    scanf("%d",&ch);
    for(i=0;i<=n;i++)
        visited[i]=0;
    switch(ch)
    {
        case 1:printf("BFS Traversal:");
        bfs(start);
        for(i=1;i<=n;i++)
        {
            if(visited[i]==0)
                printf("\nVertex %d is not reachable",i);
        }
        break;
        case 2:printf("DFS Traversal:");
        dfs(start);
        break;
        case 3:exit(0);
        default:printf("Invalid choice");
    }
    return 0;
}
